//DATABASE SCHEMA --> REPRESENTATION OF A COLLEGE WORLD

// A COLLEGE HAS SOME BRANCHES
DROP TABLE M_BRANCHES;
CREATE TABLE M_BRANCHES ( NAME VARCHAR2(256),BRANCH_CODE_PK VARCHAR2(4)  PRIMARY KEY,IS_ACTIVE BOOLEAN , VERSION_NO INT );


//EACH BRANCH HAS SUBJECTS FOR ANY SEMESTER
DROP TABLE M_SUBJECTS;
CREATE TABLE M_SUBJECTS ( SUBJECT_CODE_PK VARCHAR2(6) PRIMARY KEY, NAME VARCHAR2(512),SEMESTER INT,IS_ACTIVE BOOLEAN,VERSION_NO INT, FK_IN_BRANCH VARCHAR2(4) , FOREIGN KEY (FK_IN_BRANCH ) REFERENCES M_BRANCHES(BRANCH_CODE_PK));

// STUDENT OPT FOR ANY BRANCH
DROP TABLE M_STUDENTS;
CREATE TABLE M_STUDENTS ( STUDENT_ID_PK BIGINT AUTO_INCREMENT PRIMARY KEY, NAME VARCHAR2(256),ADDRESS VARCHAR2(512), FK_IN_BRANCH VARCHAR2(4), IS_ACTIVE BOOLEAN,VERSION_NO INT,FOREIGN KEY (FK_IN_BRANCH) REFERENCES M_BRANCHES(BRANCH_CODE_PK));


// DATA INTO TABLE --> SNAPSHOT OF THE WORLD AT ANY POINT OF TIME

// BRANCHES

